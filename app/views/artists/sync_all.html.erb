<% @newJamsByArtist.keys.each do |artist| %>

<h2>Results of sync for <%= artist.name %></h2>

<% if @newJamsByArtist[artist].any? then %>

New jams discovered:<br/>

<% @newJamsByArtist[artist].each do |jam| %>
	<%= jam.jam_creation_date.strftime("%B %e, %Y at %I:%M%p") %>: <A href="http://thisismyjam.com/<%= jam.jam_username %>"><%= jam.jam_username %></a> jammed "<%= jam.jam_title %>" <%= link_to '(remove)', :controller => 'jams', :action => 'remove', :id => jam.id %><BR/>
<% end %>

<% else %>

No new jams discovered.<br/>

<% end %>

<BR/>

<% end %>

<BR/>
<%= link_to 'Back', root_path %>
